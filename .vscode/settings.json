{
    "files.exclude": {
        ".firebase": true,
        "node_modules": true,
        "compiled": true,
        "www/[^_]*": true,
        "www/_[^a]*": true,
        "www/_a[^s]*": true,
        "www/_as[^s]*": true,
        "www/_ass[^e]*": true,
        "www/_asse[^t]*": true,
        "www/_asset[^s]*": true,
    },
    "files.trimTrailingWhitespace": true,
    "typescript.preferences.importModuleSpecifier": "auto",
    "typescript.preferences.quoteStyle": "single"
}
